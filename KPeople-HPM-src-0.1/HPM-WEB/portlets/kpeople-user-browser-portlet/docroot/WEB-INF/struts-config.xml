<?xml version="1.0"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">

<struts-config>

	<action-mappings>
	    
        <action path="/user-browser/view" type="it.webscience.kpeople.web.portlet.user.action.ViewAction">
            <forward name="/user-browser/view" path="/html/view.jsp" />
        </action>
        
		<action path="/user-browser/begin-search" type="it.webscience.kpeople.web.portlet.user.action.UserEnabledAction">
			<forward name="/user-browser/begin-search" path="/html/enabledUser.jsp"/>
		</action>
		
	</action-mappings>

</struts-config>