    <connection>kpeopler61</connection>
    <sql>SELECT &quot;Processes delay mean percentage (%)&quot;, case
when SUM(PERCENTUALE_RITARDO *100)&#47;COUNT(*) is null then 0
else SUM(PERCENTUALE_RITARDO *100)&#47;COUNT(*)
end as DELAY
FROM process p inner join time t on p.ID_TIME = t.ID_TIME
where (t.LAST_SIX_MONTH = 1);</sql>
