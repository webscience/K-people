    <connection>kpeopler61</connection>
    <sql>SELECT
  TIMESTAMP(DATE_CREATED) AS THE_DATE
, DAYNAME(DATE_CREATED)as THE_DAY
, MONTHNAME(DATE_CREATED)as THE_MONTH
, YEAR(DATE_CREATED)AS THE_YEAR
, DAY(DATE_CREATED)as DAY_OF_MONTH
, WEEK(DATE_CREATED)as WEEK_OF_YEAR
, MONTH(DATE_CREATED) AS MONTH_OF_YEAR
, 
CASE 
	WHEN QUARTER(DATE_CREATED) = 1 THEN &quot;Q1&quot;
	WHEN QUARTER(DATE_CREATED) = 2 THEN &quot;Q2&quot;
	WHEN QUARTER(DATE_CREATED) = 3 THEN &quot;Q3&quot;
	ELSE &quot;Q4&quot;
END AS QUARTER
, Case
    when datediff(CURRENT_DATE,DATE_CREATED) &lt;=30 then 1
    else 0
end AS LAST_MONTH
,
Case
 when datediff(CURRENT_DATE, DATE_CREATED) &lt;=90 then 1
  else 0
 end AS LAST_THREE_MONTH
,
Case
 when datediff(CURRENT_DATE, DATE_CREATED) &lt;=180  then 1
  else 0
 end AS LAST_SIX_MONTH
 FROM kpeopler.`process` p;</sql>
